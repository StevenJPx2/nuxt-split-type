    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@nuxt/kit": "^3.9.1",
    "split-type": "^0.3.4",
    "typescript": "^5.3.3",
    "@vueuse/nuxt": "^10.7.1"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxt/module-builder": "^0.5.5",
    "@nuxt/schema": "^3.9.1",
    "@nuxt/test-utils": "^3.9.0",
    "@types/node": "^18.19.5",
    "@vueuse/core": "^10.7.1",
    "changelogen": "^0.5.5",
    "eslint": "^8.56.0",
    "nuxt": "^3.9.1",
    "vitest": "^0.33.0"
  }
